 			if: var(Result) {
 				copyVar: var(act0), int(1)
 			} endIf: 
 			checkCurrentStateName: (PLAYER), s32'CmnActChangeEnterCutsceneRecv'
 			if: var(Result) {
 				copyVar: var(act0), int(1)
 			} endIf: 
 			checkCurrentStateName: (PLAYER), s32'CmnActHomingDash'
 			if: var(Result) {
