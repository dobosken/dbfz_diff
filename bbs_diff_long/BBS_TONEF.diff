 					} endIf: 
 				} endElse: 
 			} endIf: 
 			ifVarOfObjectTrue: (PLAYER), var(IsSomebodysCutSceneOrSelfHide) {
 				ifNot: var(act2) {
 					copyVar: var(act2), int(1)
 					copyVar: var(act3), int(1)
 				} endIf: 
 			} endIf: 
 			else:  {
 				if: var(act3) {
 					copyVar: var(act2), int(0)
 					copyVar: var(act3), int(0)
 				} endIf: 
 			} endElse: 
