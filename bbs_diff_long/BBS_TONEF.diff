 					copyVar: var(act0), int(1)
 				} endIf: 
 				else:  {
 					copyVar: var(act0), int(0)
 					checkCurrentStateName: (PLAYER), s32'AssistAttack'
 					if: var(Result) {
 						copyVar: var(act0), int(1)
 					} endIf: 
