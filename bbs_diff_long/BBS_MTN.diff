 		callSubroutine: s32'cmn_MutekiFollowAssistKinshiIni'
 	} endIf: 
 	upon: (ATTACK_COLLISION_PLAYER) {
 		clearRegisteredUponCode: (ATTACK_COLLISION_PLAYER)
 		enableVanish: 0
 		copyVar: var(act3), int(1)
 	} endUpon: 
 	upon: (ON_ATTACK_COLLISION_MAIN_PLAYER) {
 		clearRegisteredUponCode: (ON_ATTACK_COLLISION_MAIN_PLAYER)
 		triggerUponInObject: s32'mtn402_Muscle_Wind_Eff', (SIGNAL0)
 	} endUpon: 
 	upon: (IDLING) {
 		if: var(act4) {
 			checkInput: (INPUT_NOT_L)
 			if: var(Result) {
 				checkInput: (INPUT_NOT_M)
 				if: var(Result) {
 					checkInput: (INPUT_NOT_H)
 					if: var(Result) {
 						jumpToLabel: s32'atk'
 						copyVar: var(act4), int(0)
 					} endIf: 
 				} endIf: 
 			} endIf: 
 			ifNot: var(IN_BATTLE) {
 				jumpToLabel: s32'atk'
 				copyVar: var(act4), int(0)
 			} endIf: 
 			ifNot: var(IsMainPlayerSelf) {
 				jumpToLabel: s32'atk'
 				copyVar: var(act4), int(0)
 			} endIf: 
 		} endIf: 
 		if: var(act3) {
 			modifyVar: (ADD), var(act3), int(1)
 			ifOperation: (IS_GREATER), var(act3), int(10) {
 				copyVar: var(act3), int(0)
 				enableVanish: 1
 				whiffCancelOptionBufferTime: s32'CmnActMikiwameMove', 15
 			} endIf: 
 		} endIf: 
 	} endUpon: 
 	upon: (MUTEKI_PRE_HITSTOP) {
 		clearRegisteredUponCode: (MUTEKI_PRE_HITSTOP)
 		ifOperation: (IS_GREATER_OR_EQUAL), var(act0), int(20) {
 			ifOperation: (IS_LESS_OR_EQUAL), var(LoopCount), int(3) {
 				tensionUsage: 5000
 				createParticle: s32'cmn_EXstart', (CENTER)
 				cmnSfx: s32'ARC_BTL_SYS_ActSkill_Normal'
 				clearRegisteredUponCode: (TIMER)
 			} endIf: 
 		} endIf: 
 		ifNot: var(act1) {
 			copyVar: var(act1), int(1)
 			jumpToLabel: s32'full'
 			spriteOverrideFrame: s32'mtf402_04'
 			callSubroutine: s32'FullPower_ON'
 			createObject: s32'mtn402_Muscle_Wind_Eff', (ZERO)
 			cameraShake: s32'ShakeTate', 1000, 0, 35, 3
 			effectForLandBreak: 1, 1000, 0, 0
 		} endIf: 
 	} endUpon: 
 	copyVar: var(obj7), int(10)
 	upon: (ARMOR) {
 		modifyVar: (ADD), var(obj7), int(-1)
 		disableAttackClashing: 1
 		ifNot: var(obj7) {
 			clearRegisteredUponCode: (ARMOR)
 			enableSuperArmor: 0
 		} endIf: 
 	} endUpon: 
 	copyVar: var(obj3), int(0)
 } endSubroutine: 
 
