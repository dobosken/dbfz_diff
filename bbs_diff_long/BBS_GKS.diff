 beginSubroutine: s32'Kamehameha_HaseiCheck' {
 	copyVar: var(act3), int(0)
 	checkInput: (INPUT_1)
 	if: var(Result) {
 		copyVar: var(act3), int(1)
 	} endIf: 
 	checkInput: (INPUT_2)
 	if: var(Result) {
 		copyVar: var(act3), int(2)
 	} endIf: 
 	checkInput: (INPUT_3)
 	if: var(Result) {
 		copyVar: var(act3), int(3)
 	} endIf: 
 	checkInput: (INPUT_7)
 	if: var(Result) {
 		copyVar: var(act3), int(7)
 	} endIf: 
 	checkInput: (INPUT_8)
 	if: var(Result) {
 		copyVar: var(act3), int(8)
 	} endIf: 
 	checkInput: (INPUT_9)
 	if: var(Result) {
 		copyVar: var(act3), int(9)
 	} endIf: 
 	ifNot: var(act3) {
 		copyVar: var(act4), int(100)
 	} endIf: 
 	if: var(act4) {
 		modifyVarIntoResult: (SUB), var(act3), var(act4)
 		modifyVarMath: (ABSOLUTE), var(Result)
 		ifOperation: (IS_GREATER_OR_EQUAL), var(Result), int(2) {
 			copyVar: var(act4), int(100)
 			copyVar: var(act3), int(0)
 		} endIf: 
 	} endIf: 
 	else:  {
 		copyVar: var(act4), var(act3)
 	} endElse: 
 } endSubroutine: 
 
