 beginSubroutine: s32'Kijinsen_Init' {
 	callSubroutine: s32'cmnAtkLevel_3_AtkInit'
 	damage: 1100
 	groundKnockbackEffect: (AIR_FACE_UP)
 	airKnockbackEffect: (AIR_FACE_UP)
 	attackXKnockback: 35000
 	attackYKnockback: 15000
 	enemyHitstopAddition: 0, 5, 5
 	airHitstun: 16
 	guardPushback: 150
 	setInitialProration: 90
 	damageScalingOverride: 1000
 	attackDamageEffect: 4, s32'cmn_slash_s'
 	attackFinishBlowback: 1
 	damageSoundEffect: s32'ARC_BTL_GBR_Sword_Hit'
 	guardSoundEffect: s32'ARC_BTL_GBR_Sword_Guard'
 	finishCameraRequest: s32'FinishCamera_Kijinsen'
 	if: var(MediumAutocombo) {
 		addBRendaChainPD: s32'Rasetsu'
 	} endIf: 
 	if: var(IsAir) {
 		ifOperation: (IS_LESS_OR_EQUAL), var(PosY), int(40000) {
 			copyVar: var(PosY), int(40000)
 		} endIf: 
 	} endIf: 
 	jumpToStateInObject: (PARENT), s32'landing'
 	upon: (ON_ATTACK_COLLISION_MAIN_PLAYER) {
 		clearRegisteredUponCode: (ON_ATTACK_COLLISION_MAIN_PLAYER)
 		jumpToLabel: s32'end'
 	} endUpon: 
 	checkCurrentStateName: (SELF), s32'KijinsenA'
 	if: var(Result) {
 	} endIf: 
 	checkCurrentStateName: (SELF), s32'KijinsenB'
 	if: var(Result) {
 		copyVar: var(act0), int(1)
 		damage: 1200
 	} endIf: 
 	checkCurrentStateName: (SELF), s32'KijinsenC'
 	if: var(Result) {
 		copyVar: var(act0), int(1)
 		damage: 1300
 	} endIf: 
 	checkCurrentStateName: (SELF), s32'AirKijinsenA'
 	if: var(Result) {
 	} endIf: 
 	checkCurrentStateName: (SELF), s32'AirKijinsenB'
 	if: var(Result) {
 		copyVar: var(act0), int(2)
 		damage: 1200
 	} endIf: 
 	checkCurrentStateName: (SELF), s32'AirKijinsenC'
 	if: var(Result) {
 		copyVar: var(act0), int(2)
 		damage: 1300
 	} endIf: 
 	upon: (ON_ATTACK_HIT_MAIN_PLAYER_DAMAGE_PRE_CTRL) {
 		clearRegisteredUponCode: (ON_ATTACK_HIT_MAIN_PLAYER_DAMAGE_PRE_CTRL)
 		if: var(IN_BATTLE) {
 			if: var(IsMainPlayerSelf) {
 				if: var(act0) {
 					ifNot: var(SmashRegular) {
 						copyVar: var(SmashRegular), int(1)
 						copyVar: var(CameraComboOnce3), int(1)
 						copyVar: var(act1), int(1)
 						ifOperation: (IS_LESS_OR_EQUAL), var(EnemyFrontDistance), int(700000) {
 							copyVar: var(act2), int(1)
 						} endIf: 
 						ifOperation: (IS_EQUAL), var(act0), int(1) {
 							if: var(act2) {
 								atkHitPositionType: 4, 400000, 50000
 								cameraMotion: 160000, 535392, 121150, -180, -590, 0, 54, 1, 20, 15, 1
 							} endIf: 
 							else:  {
 								atkHitPositionType: 4, 750000, 50000
 								cameraMotion: 210000, 535392, 121150, -180, -590, 0, 54, 1, 20, 15, 1
 							} endElse: 
 						} endIf: 
 						else:  {
 							if: var(act2) {
 								atkHitPositionType: 4, 400000, 100000
 								cameraMotion: 160000, 535392, 121150, -180, -590, 0, 54, 1, 20, 15, 1
 							} endIf: 
 							else:  {
 								atkHitPositionType: 4, 750000, 100000
 								cameraMotion: 210000, 535392, 121150, -180, -590, 0, 54, 1, 20, 15, 1
 							} endElse: 
 						} endElse: 
 						if: var(Direction) {
 							ifOperation: (IS_LESS_OR_EQUAL), var(PosXRaw), int(-2700000) {
 								copyVar: var(PosXRaw), int(-2700000)
 							} endIf: 
 						} endIf: 
 						else:  {
 							ifOperation: (IS_GREATER_OR_EQUAL), var(PosXRaw), int(2700000) {
 								copyVar: var(PosXRaw), int(2700000)
 							} endIf: 
 						} endElse: 
 						cameraShake: s32'ShakeTateYoko', 1000, 0, 18, 1
 						hitstop: 0
 						enemyHitstopAddition: 0, 0, 0
 						upon: (ON_PLAYER_AFTER_EXECUTE_DAMAGE) {
 							clearRegisteredUponCode: (ON_PLAYER_AFTER_EXECUTE_DAMAGE)
 							ifBeginAliveEnemyPlayer: 0 {
 								superFreeze: 0, 100, 24, 25
 							} endIf: 
 						} endUpon: 
 						groundKnockbackEffect: (AIR_STRONG)
 						airKnockbackEffect: (AIR_STRONG)
 						airHitstun: 55
 						attackXKnockback: 110000
 						attackYKnockback: 15000
 						wallBounceCount: 1
 						wallBounceXVelocityPercent: 5
 						atkWallBoundWaitTime: 10
 						checkCurrentStateName: (SELF), s32'AirKijinsenB'
 						if: var(Result) {
 							atkWallBoundWaitTime: 12
 							attackYKnockback: 20000
 							airHitstun: 42
 							enemyHitstopAddition: 0, 3, 3
 						} endIf: 
 						checkCurrentStateName: (SELF), s32'KijinsenC'
 						if: var(Result) {
 							wallBounceXVelocityPercent: 30
 						} endIf: 
 						checkCurrentStateName: (SELF), s32'AirKijinsenC'
 						if: var(Result) {
 							attackYKnockback: 25000
 							wallBounceXVelocityPercent: 25
 						} endIf: 
 					} endIf: 
 				} endIf: 
 			} endIf: 
 		} endIf: 
 	} endUpon: 
