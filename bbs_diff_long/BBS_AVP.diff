 	attackGravity: 3000
 	groundKnockbackEffect: (AIR_FACE_UP)
 	airHitstun: 80
 	attackGuardtype: (HIGH)
 	attackWhiffCrouch: 1
 	atkHead: 1
 	atkFrontDirection: 0
 	atkShakeCamera: s32'ShakeTate', 700, 0, 11, 5
 	ifOperation: (IS_EQUAL), var(act0), int(10) {
 		damage: 950
 	} endIf: 
 	ifOperation: (IS_GREATER_OR_EQUAL), var(act0), int(20) {
 		damage: 1000
 	} endIf: 
 	upon: (ON_ATTACK_HIT_MAIN_PLAYER_DAMAGE_PRE_CTRL) {
 		clearRegisteredUponCode: (ON_ATTACK_HIT_MAIN_PLAYER_DAMAGE_PRE_CTRL)
 		ifVarOfObjectFalse: (ENEMY_MAIN_PLAYER), var(CmnCharaReg6) {
 			copyVar: var(act1), int(1)
 		} endIf: 
 		if: var(IN_BATTLE) {
 			if: var(IsMainPlayerSelf) {
 				if: var(act0) {
 					ifNot: var(SmashKnockdown) {
 						copyVar: var(SmashKnockdown), int(1)
 						cameraMotion: 40605, 420856, 59191, 455, 0, 0, 54, 1, 20, 15, 1
 						if: var(Direction) {
 							ifOperation: (IS_LESS_OR_EQUAL), var(PosXRaw), int(-2850000) {
 								copyVar: var(PosXRaw), int(-2850000)
 							} endIf: 
 						} endIf: 
 						else:  {
 							ifOperation: (IS_GREATER_OR_EQUAL), var(PosXRaw), int(2850000) {
 								copyVar: var(PosXRaw), int(2850000)
 							} endIf: 
 						} endElse: 
 						hitstop: 0
 						upon: (ON_PLAYER_AFTER_EXECUTE_DAMAGE) {
 							clearRegisteredUponCode: (ON_PLAYER_AFTER_EXECUTE_DAMAGE)
 							ifBeginAliveEnemyPlayer: 0 {
 								superFreeze: 0, 100, 25, 25
 							} endIf: 
 						} endUpon: 
 						damageScalingOverride: 1500
 						slidingKnockdownDuration: 25
 						groundBounceUntechTime: 5000
 						wallBounceCount: 0
 						ifOperation: (IS_GREATER_OR_EQUAL), var(act0), int(20) {
 							wallBounceCount: 0
 							groundBounceCount: 1
 							groundBounceYVelocity: 13000
 							groundBounceGravity: 10000
 							groundBounceYVelocityPercent: 100
 						} endIf: 
 						ifVarOfObjectTrue: (ENEMY_MAIN_PLAYER), var(IsAir) {
 							cameraMotion: -62354, 451620, 246387, -3259, 2347, 0, 54, 1, 20, 15, 1
 							atkHitPositionType: 4, 210000, -100000
 						} endIf: 
 					} endIf: 
 				} endIf: 
 			} endIf: 
 		} endIf: 
 	} endUpon: 
 	autoInputDirectionSearch: 1
 	atkRange: 50000000, 0, 0, 0
 	upon: (IDLING) {
 		copyVarFromObject: (SELF), var(act4), (SELF), var(SpeedY)
 		modifyVar: (MUL), var(act4), int(-1)
 		ifOperation: (IS_GREATER_OR_EQUAL), var(act4), var(PosY) {
 			copyVarFromObject: (SELF), var(PosY), (SELF), var(act4)
 			addYPosition: 1
 		} endIf: 
 	} endUpon: 
 	jumpToStateInObject: (PARENT), s32'landing'
 } endSubroutine: 
 
