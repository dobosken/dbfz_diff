 		soundOption_VoiceChannel: 1
 		forceRandomVoice: s16'vgsn000', 100, s16'vgsn001', 100, s16'vgsn002', 100, s16'vgsn003', 100
 		clearRegisteredUponCode: (IDLING)
 		clearRegisteredUponCode: (PLAYER_DAMAGE)
 		clearRegisteredUponCode: (PLAYER_GUARD)
 		clearRegisteredUponCode: (NO_STOP_IDLING)
 		jumpToLabel: s32'koware'
 		linkObjectStop: (NULL)
 		ignoreWorldStopByAll: 1
 		displayModel: 1
 	} endUpon: 
 	upon: (NO_STOP_IDLING) {
 		ifVarOfObjectFalse: (PARENT), var(IsMainPlayerSelf) {
 			checkCurrentStateName: (PLAYER), s32'AssistAttack3'
 			ifNot: var(Result) {
 				callSubroutine: s32'gsn_ClearInterrupt'
 				jumpToLabel: s32'return'
 				enableSuperArmor: 1
 				resetSpeed: 1
 				setNoCollision: 0
 				screenCollision: 0
 				resetWeightDefault: 
 				recoveryState: 
 				ignoreWorldStopByAll: 1
 			} endIf: 
 		} endIf: 
 		checkCurrentStateName: (PARENT), s32'CmnActFuttobiFinish'
 		if: var(Result) {
 			callSubroutine: s32'gsn_ClearInterrupt'
 			jumpToLabel: s32'return'
 			enableSuperArmor: 1
 			resetSpeed: 1
 			setNoCollision: 0
 			screenCollision: 0
 			resetWeightDefault: 
 			recoveryState: 
 		} endIf: 
 		checkCurrentStateName: (ENEMY_MAIN_PLAYER), s32'CmnActFuttobiFinish'
 		if: var(Result) {
 			callSubroutine: s32'gsn_ClearInterrupt'
 			jumpToLabel: s32'return'
 			enableSuperArmor: 1
 			resetSpeed: 1
 			setNoCollision: 0
 			screenCollision: 0
 			resetWeightDefault: 
 			recoveryState: 
 		} endIf: 
 		checkCurrentStateName: (PARENT), s32'CmnActLockWait'
 		if: var(Result) {
 			callSubroutine: s32'gsn_ClearInterrupt'
 			jumpToLabel: s32'return'
 			enableSuperArmor: 1
 			resetSpeed: 1
 			setNoCollision: 0
 			screenCollision: 0
 			resetWeightDefault: 
 			recoveryState: 
 			ignoreWorldStopByAll: 1
 		} endIf: 
 		if: var(PlayerDamage) {
 			setNoCollision: 0
 		} endIf: 
 	} endUpon: 
 	upon: (PLAYER_DAMAGE) {
 		deactivateObject: (SELF)
 	} endUpon: 
 	upon: (PLAYER_GUARD) {
 		deactivateObject: (SELF)
 	} endUpon: 
 	upon: (SIGNAL_NAME) {
 		checkSignalName: s32'return'
 		if: var(Result) {
 			deactivateObject: (SELF)
 		} endIf: 
 	} endUpon: 
