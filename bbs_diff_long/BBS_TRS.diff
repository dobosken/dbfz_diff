 beginSubroutine: s32'Cyclone_InputMove' {
 	upon: (IDLING) {
 		ifOperation: (IS_EQUAL), var(FrameCount), int(2) {
 			checkInput: (INPUT_ANY_UP)
 			if: var(Result) {
 				copyVar: var(act0), int(1)
 				setXSpeed: 27000
 				setYSpeed: 68000
 			} endIf: 
 		} endIf: 
 		checkInput: (INPUT_ANY_FORWARD)
 		if: var(Result) {
 			addXSpeed: 9000
 		} endIf: 
 		checkInput: (INPUT_ANY_BACK)
 		if: var(Result) {
 			addXSpeed: -9000
 		} endIf: 
 		checkInput: (INPUT_ANY_UP)
 		if: var(Result) {
 			setGravity: 1000
 			if: var(act0) {
 				setGravity: 2000
 			} endIf: 
 		} endIf: 
 		else:  {
 			setGravity: 3000
 		} endElse: 
 		checkInput: (INPUT_ANY_DOWN)
 		if: var(Result) {
 			addYSpeed: -1000
 		} endIf: 
 		if: var(act0) {
 			ifOperation: (IS_GREATER_OR_EQUAL), var(SpeedX), int(27000) {
 				setXSpeed: 27000
 			} endIf: 
 			ifOperation: (IS_LESS_OR_EQUAL), var(SpeedX), int(-27000) {
 				setXSpeed: -27000
 			} endIf: 
 		} endIf: 
 		else:  {
 			ifOperation: (IS_GREATER_OR_EQUAL), var(SpeedX), int(45000) {
 				setXSpeed: 45000
 			} endIf: 
 			ifOperation: (IS_LESS_OR_EQUAL), var(SpeedX), int(-45000) {
 				setXSpeed: -45000
 			} endIf: 
 		} endElse: 
 	} endUpon: 
 } endSubroutine: 
 
